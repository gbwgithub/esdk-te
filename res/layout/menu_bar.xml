<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/menu_bar"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:gravity="center"
    android:orientation="vertical" >

    <!-- 更多 -->

    <LinearLayout
        android:id="@+id/menu_bar_items"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal" >

        <!-- 全屏、半屏 -->

        <RelativeLayout
            android:id="@+id/audio_screen"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_state_menu_top_left"
            android:gravity="center" >

            <ImageView
                android:id="@+id/audio_screen_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_state_audio_minimum" />
        </RelativeLayout>

        <View
            android:id="@+id/line_audio_screen"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- 扩展 -->

        <RelativeLayout
            android:id="@+id/more"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_state_menu_top_left"
            android:gravity="center" >

            <ImageView
                android:id="@+id/more_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_state_extend" />
        </RelativeLayout>

        <View
            android:id="@+id/line_more"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- MIC -->

        <RelativeLayout
            android:id="@+id/video_mic"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_pad_menu_item"
            android:gravity="center" >

            <ImageView
                android:id="@+id/video_mic_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_state_mic" />
        </RelativeLayout>

        <View
            android:id="@+id/line_video_mic"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- 蓝牙，只有手机下，有 -->

        <RelativeLayout
            android:id="@+id/voice_bluetooth"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_pad_menu_item"
            android:gravity="center" >

            <ImageView
                android:id="@+id/voice_bluetooth_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_state_menu_route_bluetooth" />
        </RelativeLayout>

        <View
            android:id="@+id/line_voice_bluetooth"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- 扬声器 -->

        <RelativeLayout
            android:id="@+id/video_speaker"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_pad_menu_item"
            android:gravity="center" >

            <ImageView
                android:id="@+id/video_speaker_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_phone_menu_speaker" />
        </RelativeLayout>

        <View
            android:id="@+id/line_video_speaker"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- 查看会场列表 -->

        <RelativeLayout
            android:id="@+id/video_conf_list"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_pad_menu_item"
            android:gravity="center" >

            <ImageView
                android:id="@+id/video_conf_list_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_phone_menu_conflist" />
        </RelativeLayout>

        <View
            android:id="@+id/line_video_conf_list"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- 音频转视频 -->

        <RelativeLayout
            android:id="@+id/audio_switch_video"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_pad_menu_item"
            android:gravity="center" >

            <ImageView
                android:id="@+id/audio_switch_video_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_state_audio_switch_video" />
        </RelativeLayout>

        <View
            android:id="@+id/line_audio_switch_video"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- 共享 -->

        <RelativeLayout
            android:id="@+id/share_data"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_pad_menu_item"
            android:gravity="center" >

            <ImageView
                android:id="@+id/share_data_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_state_share" />
        </RelativeLayout>

        <View
            android:id="@+id/line_share_data"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- 二次拨号盘 -->

        <RelativeLayout
            android:id="@+id/redial_board"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_pad_menu_item"
            android:gravity="center" >

            <ImageView
                android:id="@+id/redial_board_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_state_dial" />
        </RelativeLayout>

        <View
            android:id="@+id/line_redial_board"
            android:layout_width="1dp"
            android:layout_height="fill_parent"
            android:background="@drawable/te_line_portrait" />
        <!-- 挂断 -->

        <RelativeLayout
            android:id="@+id/video_hangup"
            android:layout_width="90.50dp"
            android:layout_height="51.50dp"
            android:layout_gravity="center"
            android:background="@drawable/te_state_menu_top_right"
            android:gravity="center" >

            <ImageView
                android:id="@+id/video_hangup_img"
                android:layout_width="90dp"
                android:layout_height="52dp"
                android:layout_centerInParent="true"
                android:layout_marginLeft="0.50dp"
                android:duplicateParentState="true"
                android:src="@drawable/te_state_hangup" />
        </RelativeLayout>
    </LinearLayout>

    <!-- 底部 -->

    <View
        android:id="@+id/line_menu_bar_bottom"
        android:layout_width="fill_parent"
        android:layout_height="1dp"
        android:layout_alignParentTop="true"
        android:background="@drawable/te_line_crosswise" />

    <RelativeLayout
        android:id="@+id/menu_bar_bottom"
        android:layout_width="match_parent"
        android:layout_height="34.50dp"
        android:background="@drawable/te_state_menu_bottom_left_right_normal"
        android:gravity="center_horizontal" >

        <LinearLayout
            android:id="@+id/remote_number_layout"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:layout_marginRight="17dp"
            android:layout_marginTop="0.50dp"
            android:gravity="right|center_vertical"
            android:orientation="horizontal"
            android:visibility="gone" >

            <TextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:paddingRight="3.75dp"
                android:text="@string/remote_number"
                android:textColor="#9c9c9c"
                android:textSize="13sp" />

            <TextView
                android:id="@+id/remote_number"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textColor="#9c9c9c"
                android:textSize="13sp" />
        </LinearLayout>

        <LinearLayout
            android:id="@+id/titlemenu"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_alignParentLeft="true"
            android:layout_marginLeft="0.50dp"
            android:layout_marginTop="0.50dp"
            android:gravity="left|center_vertical"
            android:orientation="horizontal" >

            <ImageView
                android:id="@+id/vedio_menu_single"
                android:layout_width="90dp"
                android:layout_height="30dp"
                android:layout_marginTop="2dp" />

            <TextView
                android:id="@+id/audio_time"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="20.25dp"
                android:gravity="center"
                android:text="@string/zero_begin_time"
                android:textColor="#9c9c9c"
                android:textSize="13sp"
                android:visibility="invisible" />
        </LinearLayout>

        <TextView
            android:id="@+id/audio_time1"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:gravity="center"
            android:text="@string/zero_begin_time"
            android:textColor="#898989"
            android:textSize="10.50dp"
            android:textStyle="bold"
            android:visibility="gone" />

        <RelativeLayout
            android:id="@+id/callcoming_signal_btn"
            android:layout_width="0dp"
            android:layout_height="0dp"
            android:layout_alignParentRight="true"
            android:visibility="gone" >

            <ImageView
                android:id="@+id/vedio_menu_single1"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:visibility="gone" />
        </RelativeLayout>
    </RelativeLayout>

</LinearLayout>